{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/my-home-app/favicon.ico",
          "/my-home-app/index.html",
          "/my-home-app/manifest.webmanifest",
          "/my-home-app/*.css",
          "/my-home-app/*.js"
        ],
        "urls": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/my-home-app/assets/**",
          "/my-home-app/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ],
        "urls": [
          "/fontawesome-webfont*"
        ]
      }
    }
  ]
}
